    "@radix-ui/react-slot": "^1.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.2",
    "@cloudflare/vite-plugin": "1.14.0",
